---
import EditorComponent from "@/components/Editor.tsx";
import { languages } from "@/i18n/ui";
import StudioLayout from "@/layouts/StudioLayout.astro";

export function getStaticPaths() {
	return Object.keys(languages).map((lang) => ({ params: { lang } }));
}

const { lang } = Astro.params;
---

<StudioLayout
	title="Logo Studio Editor"
	description="Edit and export your logo."
	lang={lang}
>
	<EditorComponent client:only="react" lang={lang} />
</StudioLayout>
